using System;

namespace ExerciseDay1
{
    class Program
    {

        static void Main(string[] args)
        {//1990-2500所有闰年
            for (int i=1990;i<=2500;i++)
            {
                if ((i%4==0&&i%100!=0)||i%400==0)
                {
                    Console.WriteLine("{0}是闰年", i);
                      
                }
            }
            Console.ReadKey();
        }
        
    }
}

   
