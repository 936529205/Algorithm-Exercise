using System;

namespace ExerciseDay1
{
    class Program
    {
        static void Main(string[] args)
        {   //求1-100的和
            int i ;
            int sum = 0;
            for (i = 1; i <= 100; i++)
            {
                sum += i;
            }
            Console.WriteLine("1-100的和是{0}", sum);
            Console.ReadKey();
        }
    }

}
   
